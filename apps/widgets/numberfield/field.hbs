
<div class="bl-widget input-field" data-type="numberfield" data-key="{{config.key}}">
	<input value="\{{quote  {{config.hbsValidName}} }}" id="{{safename config.name}}_\{{safename @root._id}}" name="{{config.name}}" type="number" class="validate" style="background-color:#eee;padding-left:5px;" {{#if config.min}}min="{{config.min}}"{{/if}} {{#if config.max}}max="{{config.max}}"{{/if}}>
	<label for="{{safename config.name}}_\{{safename @root._id}}">{{config.fieldLabel}}</label>
	\{{#if "{{quote config.fieldDescription}}" }}
		<span class="field-description bl-textfield">{{config.fieldDescription}}</span>
	\{{/if}}
</div>



<div class="bl-widget input-field bl-init" data-type="datefield" data-key="{{config.key}}">
	<label for="{{safename config.name}}_\{{safename @root._id}}">{{config.fieldLabel}}</label>
	<input  value="\{{  {{config.hbsValidName}} }}" id="{{safename config.name}}_\{{safename @root._id}}" name="{{config.name}}" type="text" class="datepicker" style="background-color:#eee;padding-left:5px;">
	\{{#if "{{quote config.fieldDescription}}" }}
		<span class="field-description bl-textfield">{{config.fieldDescription}}</span>
	\{{/if}}
</div>

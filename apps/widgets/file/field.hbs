
<div class="bl-widget bl-init" style="margin-bottom:25px;" data-type="file" data-key="{{config.key}}">
	<input id="{{safename config.name}}_\{{safename @root._id}}" name="{{config.name}}" type="file" class="validate" style="background-color:#eee;padding-left:5px;">
	<label for="{{safename config.name}}_\{{safename @root._id}}">{{config.fieldLabel}}</label>
	\{{#if "{{quote config.fieldDescription}}" }}
		<span class="field-description bl-textfield">{{config.fieldDescription}}</span>
	\{{/if}}
</div>

